{
  "name": "lmorandomizer",
  "description": "Randomizer for La-Mulana Original.",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/progre/lmoriginalrandomizer/issues"
  },
  "build": {
    "directories": {
      "output": "output"
    },
    "files": [
      "dist",
      "LICENSE",
      "README.md"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "iconv-lite": "^0.4.24",
    "js-sha3": "^0.8.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@progre/tslint-config": "~5.10.0",
    "@progre/webpack-config": "^0.0.6",
    "@types/node": "^8.10.27",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "del-cli": "^1.1.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.15.1",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "source-map-support": "^0.5.9",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^2.9.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "homepage": "https://github.com/progre/lmoriginalrandomizer#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/progre/lmoriginalrandomizer.git"
  },
  "scripts": {
    "build": "run-s clean release-build",
    "clean": "del-cli dist tmp",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "debug-build": "webpack --mode development --progress",
    "dev": "webpack --mode development --progress --watch",
    "package": "electron-builder --win portable --mac zip --publish never",
    "release-build": "webpack --mode production --progress",
    "start": "electron ."
  },
  "typings": "dist/index.d.ts"
}
